{
    "parser": "babel-eslint",
    "env": {
        "es6": true,
        "browser": true,
        "node": true
    },
    "rules": {
        "semi":"off",
        "arrow-parens":"off",
        "comma-dangle":"off",
        "require-jsdoc":"off",
        "indent":"off",
        "linebreak-style": 0,
        "quotes":"off",
        "operator-linebreak":"off"
    },
    "plugins": ["jest"],
    "extends": ["eslint:recommended", "google"]
}